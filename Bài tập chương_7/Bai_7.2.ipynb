    "import tkinter as tk\n",
    "\n",
    "window = tk.Tk()\n",
    "\n",
    "window.title(\"Cửa sổ Tkinter với Nhãn\")\n",
    "\n",
    "window.geometry(\"400x300\") \n",
    "\n",
    "\n",
    "label = tk.Label(window, text=\"Chào mừng đến với thế giới của n_thai\", font=(\"Arial\", 14))\n",
    "label.pack(pady=20)  \n",
    "\n",
    "window.mainloop()\n"
   ]
  }
